{
  "name": "giveme-resume",
  "description": "Simple resume API and generator built with Fastify",
  "version": "1.0.0",
  "main": "app.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=test node --test test/**/*.test.js",
    "start": "fastify start -o -l info app.js",
    "dev": "fastify start -o -w -l info -P app.js"
  },
  "keywords": ["fastify", "node.js", "resume", "generators"],
  "author": {
    "name": "Mel Kristian Cruz",
    "email": "greetings@melkristian.com",
    "url": "https://melkristian.com"
  },
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^5.0.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.0.0",
    "fastify-cli": "^6.0.0",
    "fastify-plugin": "^4.0.0",
    "lowdb": "^7.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "c8": "^7.13.0",
    "commander": "^11.1.0"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}
